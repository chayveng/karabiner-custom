{
  "title": "AAA",
  "maintainers": [
    "Pinoxchio"
  ],
  "rules": [
    {
      "description": "hole button5 => left_control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5",
            "modifiers": {}
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "pointing_button": "button5"
            }
          ]
        }
      ]
    },
    {
      "description": "hole button4 => left_command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4",
            "modifiers": {}
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "to_if_alone": [
            {
              "pointing_button": "button4"
            }
          ]
        }
      ]
    },
    {
      "description": "<== Screen",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "==> Screen",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "button3 to command + w",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button3",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "button3 to command + shift + T",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button3",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "t",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}