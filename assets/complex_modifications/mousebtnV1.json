{
  "title": "MouseBTN",
  "maintainers": [
    "Pinoxchio"
  ],
  "rules": [
    {
      "description": "button5 + button1 to left_control + left_arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "pointing_button": "button5"
              },
              {
                "pointing_button": "button1"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "button5 + button2 to left_control + right_arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "pointing_button": "button5"
              },
              {
                "pointing_button": "button2"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "test",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button3",
            "modifiers": {
              "mandatory": [],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    }
  ]
}